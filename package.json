{
  "name": "mock-serverify",
  "displayName": "Mock serverify",
  "version": "0.0.1",
  "description": "useful mock extension",
  "author": "Plasmo Corp. <foss@plasmo.com>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "test": "plasmo test"
  },
  "dependencies": {
    "plasmo": "0.84.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.21.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.251",
    "@types/node": "20.9.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "autoprefixer": "^10.4.16",
    "daisyui": "^4.4.20",
    "postcss": "^8.4.32",
    "prettier": "3.0.3",
    "tailwindcss": "^3.3.7",
    "typescript": "5.2.2"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "scripting",
      "storage",
      "activeTab",
      "tabs"
    ],
    "web_accessible_resources": [
      {
        "matches": [
          "<all_urls>"
        ],
        "resources": [
          "tabs/delta-flyer.html"
        ]
      }
    ]
  }
}
