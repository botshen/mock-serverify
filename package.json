{
  "name": "mock-serverify",
  "displayName": "Mock serverify",
  "version": "0.0.1",
  "description": "useful mock extension",
  "author": "Plasmo Corp. <foss@plasmo.com>",
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "svgo": "2.8.0",
        "node-fetch": "3.3.1"
      }
    },
    "allowedDeprecatedVersions": {
      "har-validator": "5.1.5",
      "request": "2.88.2",
      "stable": "0.1.8",
      "uuid": "3.4.0"
    }
  },
  "scripts": {
    "dev": "plasmo dev",
    "dev:firefox": "plasmo dev --target=firefox-mv2",
    "start": "web-ext run --source-dir ./build/chrome-mv3-dev -t chromium --start-url chrome://newtab",
    "start:firefox": "web-ext run --source-dir ./build/firefox-mv2-dev",
    "debug": "plasmo dev --verbose",
    "debug:firefox": "plasmo dev --target=firefox-mv2 --verbose",
    "build": "plasmo build",
    "build:firefox": "plasmo build --target=firefox-mv2",
    "package": "plasmo package",
    "package:firefox": "plasmo package --target=firefox-mv2",
    "test": "plasmo test"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@plasmohq/messaging": "^0.6.1",
    "@plasmohq/storage": "^1.9.0",
    "ajax-hook": "^3.0.3",
    "flatted": "^3.2.9",
    "formik": "^2.4.5",
    "framer-motion": "^10.16.16",
    "hook": "link:@plasmohq/storage/hook",
    "plasmo": "0.84.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.21.0",
    "url-parse": "^1.5.10",
    "vanilla-jsoneditor": "^0.21.1",
    "web-ext": "^7.9.0",
    "webextension-polyfill": "^0.10.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.251",
    "@types/node": "20.9.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "prettier": "3.0.3",
    "tailwindcss": "^3.3.7",
    "typescript": "5.2.2"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "scripting",
      "storage",
      "activeTab",
      "tabs"
    ],
    "web_accessible_resources": [
      {
        "matches": [
          "<all_urls>"
        ],
        "resources": [
          "tabs/delta-flyer.html"
        ]
      }
    ]
  }
}
